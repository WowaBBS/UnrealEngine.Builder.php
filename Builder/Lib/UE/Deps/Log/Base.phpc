<?  $Loader->Parent_Class('/Object');    class C_UE_Deps_Log_Base Extends C_Object  {    Var $Dir='Log';      Function _Save($LogName, $Data)    {      $Dir=$this->Dir;      if(!Is_Dir($Dir))        CreatePath($Dir);      $FileLog=$Dir.'/'.$LogName.'.log';      File_Put_Contents($FileLog, $Data);    }        Function Clear() {}    Function Save() {}        Function _Done()    {      $this->Save();      $this->Clear();      Parent::_Done();    }  }?>