<?  $Loader->Parent_Class('/UE/Deps/Log/Base');    class C_UE_Deps_Log_ClassLink extends C_UE_Deps_Log_Base  {    Var $List=[];        Function Add($FromPath, $Class)    {      $this->List[$FromPath][$Class]=true;    }        Function Save()    {      $Res=[];      Array_Push($Res, "ClassLink only list:\n\n");      ForEach($this->List As $From=>$Classes)      {        Array_Push($Res, '  From ',$From, " Classes: \n");        ForEach($Classes As $Class=>$tmp)          Array_Push($Res, '    ', $Class, "\n");        Array_Push($Res, "\n");      }      $this->_Save('ClassLink', Implode($Res));    }        Function Clear()    {      parent::Clear();      $this->List=[];    }  }?>